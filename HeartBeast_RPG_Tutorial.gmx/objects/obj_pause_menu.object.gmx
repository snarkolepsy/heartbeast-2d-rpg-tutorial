        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Control the Menu

if (alarm[0] &lt;= 0) { // alarm allows smoother scrolling with gamepad

    // scrolling through with down key
    if (obj_input.down_key) {
        // move down the options list, if available
        if (menu_index &lt; array_length_1d(option) - 1) {
            menu_index++;
        }
        // loop back around the menu options when we go past the last one
        else {
            menu_index = 0;
        }
        // reset the alarm
        alarm[0] = room_speed/6;
    }
    
    // scrolling through with the up key
    if (obj_input.up_key) {
        // move up the options list, if available
        if (menu_index &gt; 0) {
            menu_index--;
        }
        // if at the top, loop back around
        else {
            menu_index = array_length_1d(option) - 1;
        }
        // reset the alarm
        alarm[0] = room_speed/6;
    }
    
    // check for dash key as context-sensitive confirm/select key
    if (obj_input.dash_key) {
        switch (menu_index) {
            case 0: // return to the game
                room_start_action = RETURN;
                room_goto(obj_player_stats.previous_room);
                break;
            case 1: // save the game
                scr_save_game();
                break;
            case 2: // load the game
                obj_player_stats.room_start_action = LOAD;
                room_goto(obj_player_stats.previous_room);
                break;
            case 3: // exit the game
                game_end();
                break;
            default:
                break;
        }
        // prevent player from dashing after selecting "Return"
        obj_input.dash_key = false;
    }
}
</string>
